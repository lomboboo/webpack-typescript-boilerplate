{
  "name": "js-boilerplate",
  "version": "1.0.0",
  "description": "JavaScript development environment by @lomboboo",
  "scripts": {
    "start": "npm-run-all --parallel security-check webpack-dev-server eslint:watch",
    "webpack-dev-server": "webpack-dev-server --content-base src/ --port 8000",
    "security-check": "nsp check",
    "share": "lt --port 80 --subdomain lomboboo",
    "eslint": "esw webpack.config.* src build --color",
    "eslint:watch": "npm run eslint -- --watch"
  },
  "author": "Roman Prokopchuk",
  "license": "MIT",
  "dependencies": {
    "webpack-dev-server": "^1.16.2",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
